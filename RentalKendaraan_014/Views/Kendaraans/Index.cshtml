@model IEnumerable<RentalKendaraan_014.Models.Kendaraan>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NamaKendaraan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.N0Polisi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.N0Stnk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdJenisKendaraan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ketersediaan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaKendaraan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.N0Polisi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.N0Stnk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdJenisKendaraan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ketersediaan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdKendaraan">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdKendaraan">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdKendaraan">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
